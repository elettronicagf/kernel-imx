//Replace UART1 with this node for RS-485
&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1_rs485>;
	rts-gpios = <&gpio1 18 GPIO_ACTIVE_HIGH>;
	cts-gpios = <&gpio1 19 GPIO_ACTIVE_HIGH>;
	linux,rs485-enabled-at-boot-time;
	linux,rs485-rts-on-send;
	status = "okay";
};

&uart2 {
	fsl,uart-has-rtscts;
	status = "okay";
};

&uart3 {
	status = "okay";
};

&uart5 {
	status = "okay";
};

&usbotg1 {
	vbus-supply = <&reg_usb_otg1_vbus>;
	dr_mode = "otg";
	disable-over-current;
	status = "okay";
};

&usbotg2 {
	vbus-supply = <&reg_usb_otg2_vbus>;
	dr_mode = "host";
	disable-over-current;
	status = "okay";
};

//eMMC
&usdhc1 {
	cd-gpios = <>;
	wp-gpios = <>;
	vmmc-supply = <>;
	bus-width = <4>;
	non-removable;
	no-1-8-v;
	status = "okay";
};

//wifi atwilc3000
&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2>;
	bus-width = <4>;
	non-removable;
	no-1-8-v;
	status = "okay";
	
    keep-power-in-suspend;
    enable-sdio-wakeup;
	non-removable;    /* non-removable is not a variable, the fact it is */
	                  /* listed is all that is used by driver  */
	cap-power-off-card;
	pm-ignore-notify;	
	
	#address-cells = <1>;
	#size-cells = <0>;	
	
	//wireless Microchip ATWILC3000
    wifi_wilc_sdio: wifi_wilc_sdio@1 {
        compatible = "microchip,wilc1000", "microchip,wilc3000", "atmel,wilc_sdio";
        reset-gpios = <&gpio5 4 0>;
        chip_en-gpios = <&gpio5 6 0>;
        irq-gpios = <&gpio5 5 0>;
        status = "okay";
        reg = <1>;
        bus-width = <4>;
    };	
};

&wdog1 {
	fsl,wdog_b;
};
