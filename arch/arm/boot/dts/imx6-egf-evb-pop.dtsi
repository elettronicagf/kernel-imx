#include <dt-bindings/input/input.h>
#include <dt-bindings/interrupt-controller/irq.h>

/ {
	aliases {
		mxcfb0 = &mxcfb1;
		lvds_channel0 = &lvds_channel0;
		lvds_channel0_timings = &lvds_channel0_timings;
		EGF_BLC1093 = &EGF_BLC1093;
	};

	chosen {
		stdout-path = &uart1;
	};

	memory: memory {
		reg = <0x10000000 0x40000000>;
	};

	gpio-keys {
		compatible = "gpio-keys";
		power {
			label = "Power Button";
			gpios = <&gpio5 7 1>;
			linux,code = <KEY_POWER>;
			gpio-key,wakeup;
		};
	};

	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		reg_usb_otg_vbus: regulator@0 {
			compatible = "regulator-fixed";
			reg = <0>;
			gpio = <&gpio2 5 0>;
			regulator-name = "usb_otg_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			enable-active-high;
		};
		
		reg_usb_h1_vbus: regulator@1 {
			compatible = "regulator-fixed";
			reg = <1>;
			gpio = <&gpio2 1 0>;
			regulator-name = "usb_h1_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			enable-active-high;
		};
		
		
		reg_lcd_vdd_dummy: regulator@3 {
			compatible = "regulator-fixed";
			reg = <3>;
			regulator-name = "lcd_vdd";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
		};

		
		reg_eth_vdd: regulator@5 {
			compatible = "regulator-fixed";
			reg = <5>;
			regulator-name = "eth_vdd";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
		};		
		
		reg_camera_dummy: regulator@6 {
			compatible = "regulator-fixed";
			reg = <6>;
			regulator-name = "camera-supplies";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
		};	
		
		reg_wlan: regulator@7 {
			compatible = "regulator-fixed";
			reg = <7>;
			regulator-name = "wlan-supply";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			gpio = <&gpio6 10 0>;
			enable-active-high;
			regulator-boot-on;
			regulator-always-on;
		};
		
		reg_wlan_en: regulator@8 {
			compatible = "regulator-fixed";
			reg = <8>;
			regulator-name = "wlan-en";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			startup-delay-us = <200000>;
			vin-supply = <&reg_wlan>;
			gpio = <&gpio3 19 0>;
			enable-active-high;
		};
		
		reg_audio_dummy: regulator@9 {
			compatible = "regulator-fixed";
			reg = <9>;
			regulator-name = "audio_vdda_vddio";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
		};
		
		reg_1v8_audio_dummy: regulator@10 {
			compatible = "regulator-fixed";
			reg = <10>;
			regulator-name = "audio_vddd";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			regulator-always-on;		
		};

	};
	
	mxcfb1: fb@0 {
		compatible = "fsl,mxc_sdc_fb";
		disp_dev = "hdmi";
		interface_pix_fmt = "RGB24";
		mode_str ="1920x1080M@60";
		default_bpp = <24>;
		int_clk = <0>;
		late_init = <0>;
		status = "okay";
	};
	
	mxcfb2: fb@1 {
		compatible = "fsl,mxc_sdc_fb";
		disp_dev = "ldb";
		interface_pix_fmt = "RGB666";
		default_bpp = <18>;
		int_clk = <0>;
		late_init = <0>;
		status = "okay";
	};
	
	v4l2_cap_0 {
		compatible = "fsl,imx6q-v4l2-capture";
		ipu_id = <0>;
		csi_id = <0>;
		mclk_source = <0>;
		status = "okay";
	};

	v4l2_out {
		compatible = "fsl,mxc_v4l2_output";
		status = "okay";
	};

	sound {
		compatible = "fsl,imx-egf-pop-sgtl5000",
			     "fsl,imx-audio-sgtl5000";
		model = "imx-egf-pop-sgtl5000";
		ssi-controller = <&ssi1>;
		audio-codec = <&codec>;
		audio-routing =
			"Line Out Jack", "LINE_OUT",
			"MIC_IN", "Mic Jack",
			"Mic Jack", "Mic Bias";		
		mux-int-port = <1>;
		mux-ext-port = <5>;
	};
	
	sound-hdmi {
		compatible = "fsl,imx6q-audio-hdmi",
			     "fsl,imx-audio-hdmi";
		model = "imx-audio-hdmi";
		hdmi-controller = <&hdmi_audio>;
	};

	sound-spdif {
		compatible = "fsl,imx-audio-spdif",
			   "fsl,imx-egf-spdif";
		model = "imx-spdif";
		spdif-controller = <&spdif>;
		spdif-in;
		spdif-out;
	};

	leds {
		compatible = "gpio-leds";
		led_blue {
			label = "UserLEDBlue";
			gpios = <&gpio5 4 0>;
			linux,default-trigger = "heartbeat";
		};
		led_red {
			label = "UserLEDRed";
			gpios = <&gpio3 5 0>;
			linux,default-trigger = "none";
		};
		led_green {
			label = "UserLEDGreen";
			gpios = <&gpio3 4 0>;
			linux,default-trigger = "none";
		};
	};
	
};

&cpu0 {
	operating-points = <
	/* kHz    uV */
		1200000 1275000
		996000  1250000
		852000  1250000
		792000  1175000
		396000  1075000
	>;
	fsl,soc-operating-points = <
	/* ARM kHz  SOC-PU uV */
		1200000 1275000
		996000	1275000
		852000	1275000
		792000	1275000
		396000	1275000
	>;
};

&snvs_poweroff {
	status = "okay";
};

&audmux {
	status = "okay";
};

&ssi1 {
	fsl,mode = "i2s-slave";
	status = "okay";
};

&dcic1 {
	dcic_id = <0>;
	dcic_mux = "dcic-hdmi";
	status = "okay";
};

&hdmi_audio {
	status = "okay";
};

&hdmi_cec {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hdmi_cec>;
	status = "okay";
};

&hdmi_core {
	ipu_id = <0>;
	disp_id = <1>;
	status = "okay";
};

&hdmi_video {
	fsl,phy_reg_vlev = <0x0294>;
	fsl,phy_reg_cksymtx = <0x800d>;
	status = "okay";
};

&spdif {
	pinctrl-names = "default";
	pinctrl-0 = <>;
	assigned-clocks = <&clks IMX6QDL_CLK_SPDIF_SEL>,
			  <&clks IMX6QDL_CLK_SPDIF_PODF>;
	assigned-clock-parents = <&clks IMX6QDL_CLK_PLL3_PFD3_454M>;
	assigned-clock-rates = <0>, <227368421>;
	status = "okay";
};

&ldb {
	status = "okay";
	enable-active-high;

	lvds_channel0: lvds-channel@0 {
		fsl,data-mapping = "spwg";
		fsl,data-width = <18>;
		crtc = "ipu1-di0";
		status = "okay";
		primary;

		lvds_channel0_timings: display-timings {
			native-mode = <&EGF_BLC1093>;
			EGF_BLC1093: kwh070kq13-f02 {
				clock-frequency = <40000000>;
				hactive = <800>;
				vactive = <480>;
				hback-porch = <45>;
				hfront-porch = <210>;
				vback-porch = <22>;
				vfront-porch = <132>;
				hsync-len = <1>;
				vsync-len = <1>;
			};
		};
	};
};

&can2 {
	status = "okay";
};

&pwm1 {
	status = "okay";
};

&pwm2 {
	status = "okay";
};

&cpu0 {
	arm-supply = <&sw1a_reg>;
	soc-supply = <&sw1c_reg>;
};

&gpc {
	//use ldo-bypass, u-boot will check it and configure
	fsl,ldo-bypass = <1>;
};

&iomuxc {
	pinctrl-names = "default";
	imx6q-egf {
		pinctrl_hdmi_cec: hdmicecgrp {
			fsl,pins = <
				MX6QDL_PAD_KEY_ROW2__HDMI_TX_CEC_LINE 0x1f8b0
			>;
		};

		pinctrl_enet: enetgrp {
			fsl,pins = <
				MX6QDL_PAD_ENET_MDIO__ENET_MDIO		0x1b0b0
				MX6QDL_PAD_ENET_MDC__ENET_MDC		0x1b0b0
				MX6QDL_PAD_ENET_CRS_DV__GPIO1_IO25	0x130b0		/* AR8035 reset */
				MX6QDL_PAD_ENET_RXD1__GPIO1_IO26	0x80000000	/* AR8035 interrupt */
				MX6QDL_PAD_RGMII_TXC__RGMII_TXC		0x1b0b0
				MX6QDL_PAD_RGMII_TD0__RGMII_TD0		0x1b0b0
				MX6QDL_PAD_RGMII_TD1__RGMII_TD1		0x1b0b0
				MX6QDL_PAD_RGMII_TD2__RGMII_TD2		0x1b0b0
				MX6QDL_PAD_RGMII_TD3__RGMII_TD3		0x1b0b0
				MX6QDL_PAD_RGMII_TX_CTL__RGMII_TX_CTL	0x1b0b0
				MX6QDL_PAD_ENET_REF_CLK__ENET_TX_CLK	0x1b0b0
				MX6QDL_PAD_RGMII_RXC__RGMII_RXC		0x1b0b0
				MX6QDL_PAD_RGMII_RD0__RGMII_RD0		0x1b0b0
				MX6QDL_PAD_RGMII_RD1__RGMII_RD1		0x1b0b0
				MX6QDL_PAD_RGMII_RD2__RGMII_RD2		0x1b0b0
				MX6QDL_PAD_RGMII_RD3__RGMII_RD3		0x1b0b0
				MX6QDL_PAD_RGMII_RX_CTL__RGMII_RX_CTL	0x1b0b0
			>;
		};
	};
};

&ecspi4 {
	fsl,spi-num-chipselects = <1>;
	cs-gpios = <&gpio5 2 0>;
	status = "okay";

	flash: m25p80@0 {
		compatible = "sst,s25fl116k";
		spi-max-frequency = <20000000>;
		reg = <0>;
		#address-cells = <1>;
		#size-cells = <1>;
		mtd0@00000000 {
			label = "SPL";
			reg = <0x000000 0x040000>;
		};
		mtd1@00040000 {
			label = "UBOOT";
			reg = <0x040000 0x100000>;
		};
		mtd2@00140000 {
			label = "DATA";
			reg = <0x140000 0x0C0000>;
		};

	};
};

&ecspi3 {
	fsl,spi-num-chipselects = <4>;
	cs-gpios = <
		&gpio4 24 0
		&gpio4 25 0
		&gpio4 26 0
		&gpio4 27 0
	>;
	status = "okay";
	
	sx8652@0 {
		compatible = "semtech,sx8652";
		interrupt-parent = <&gpio6>;   //GPIO6_IO0
		interrupts = <0 2>;
		spi-max-frequency = <1000000>;
		reg = <0>;
		x-min = /bits/ 16 <150>;
		x-max = /bits/ 16 <3830>;
		y-min = /bits/ 16 <190>;
		y-max = /bits/ 16 <3830>;
		x-plate-ohms = /bits/ 16 <450>;
		y-plate-ohms = /bits/ 16 <250>;
		pressure-max = /bits/ 16 <500>;
		pendown-gpio = <&gpio6 0 1>;
		ts-wires = /bits/ 8 <4>;
	};
	
	spidev0: spi@2 {
		compatible = "egf,0556spidev";
		reg = <2>;
		spi-max-frequency = <24000000>;
	};
	spidev1: spi@3 {
		compatible = "egf,0556spidev";
		reg = <3>;
		spi-max-frequency = <24000000>;
	};
};

&i2c1 {
	clock-frequency = <100000>;
	status = "okay";
	
	//rtc pcf85063
	pcf85063@51 {
		compatible = "nxp,pcf85063";
		reg = <0x51>;
	};

	codec: sgtl5000@0a {
		compatible = "fsl,sgtl5000";
		reg = <0x0a>;
		clocks = <&clks 201>;
		VDDA-supply = <&reg_audio_dummy>;
		VDDIO-supply = <&reg_audio_dummy>;
		VDDD-supply = <&reg_1v8_audio_dummy>;
	};
	
	// eeprom factory data (carrier)
	24c64@54 {
		compatible = "fsl,24c64";
		reg = <0x54>;
		pagesize = <32>;
	};
	
	ov5640_mipi: ov5640_mipi@3c { /* i2c2 driver */
		compatible = "ovti,ov564x_mipi";
		reg = <0x3c>;
		clocks = <&clks 201>;
		clock-names = "csi_mclk";
		DOVDD-supply = <&reg_camera_dummy>; /* 1.8v */
		AVDD-supply = <&reg_camera_dummy>;  /* 2.8v,*/
		DVDD-supply = <&reg_camera_dummy>;  /* 1.5v */
		pwn-gpios = <&gpio2 8 1>;   /* active low   */
		rst-gpios = <&gpio2 11 0>;   /* active high */
		csi_id = <0>;
		mclk = <24000000>;
		mclk_source = <0>;
	};
	
};

&i2c2 {
	clock-frequency = <100000>;
	status = "okay";
	
	//eeprom factory data (som)
	24c64@50 {
		compatible = "fsl,24c64";
		reg = <0x50>;
		pagesize = <32>;
	};
	
	pmic: pfuze100@08 {
		compatible = "fsl,pfuze100";
		reg = <0x08>;

		regulators {
			sw1a_reg: sw1ab {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw1c_reg: sw1c {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw2_reg: sw2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw3a_reg: sw3a {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw3b_reg: sw3b {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw4_reg: sw4 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};

			swbst_reg: swbst {
				regulator-min-microvolt = <5000000>;
				regulator-max-microvolt = <5150000>;
			};

			snvs_reg: vsnvs {
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <3000000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vref_reg: vrefddr {
				regulator-boot-on;
				regulator-always-on;
			};

			vgen1_reg: vgen1 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen2_reg: vgen2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen3_reg: vgen3 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
			};

			vgen4_reg: vgen4 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
			};

			vgen5_reg: vgen5 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
			};

			vgen6_reg: vgen6 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
			};
		};
	};
};

&i2c3 {
	clock-frequency = <100000>;
	status = "okay";
	
	hdmi: edid@50 {
		compatible = "fsl,imx6-hdmi-i2c";
		reg = <0x50>;
	};
};

// UART1 - 2 fili 
&uart1 {
	status = "okay";
};

// UART2 - 4 fili 
&uart2 {
	fsl,uart-has-rtscts;
	status = "okay";
};

// UART3 - 4 fili 
&uart3 {
	fsl,uart-has-rtscts;
	status = "okay";
};

// UART4 - 2 fili 
&uart4 {
	status = "okay";
};

// UART5 - 2 fili 
&uart5 {
	status = "okay";
};

// microSD
&usdhc1 {
	pinctrl-names = "default";
	pinctrl-0 = <>;
	bus-width = <4>;
	no-1-8-v;
	cd-gpios = <&gpio6 8 1>;
	status = "okay";
};

//eMMC Flash Interna
&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <>;
	bus-width = <4>;
	non-removable;
	no-1-8-v;
	keep-power-in-suspend;
    enable-sdio-wakeup;
    cap-power-off-card;
    vmmc-supply = <&reg_wlan_en>;
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	wlcore: wlcore@2 { 
		compatible = "ti,wl1837";
		reg = <2>;
		interrupt-parent = <&gpio6>;
		interrupts = <5 IRQ_TYPE_LEVEL_HIGH>;
	};
};


//eMMC Flash Interna
&usdhc3 {
	pinctrl-names = "default";
	pinctrl-0 = <>;
	bus-width = <8>;
	non-removable;
	no-1-8-v;
	status = "okay";
};

&fec {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_enet>;
	phy-mode = "rgmii";
	phy-reset-gpios = <&gpio1 25 0>;
	fsl,magic-packet;
	status = "okay";
};

&usbotg {
	vbus-supply = <&reg_usb_otg_vbus>;
	disable-over-current;
	srp-disable;
	hnp-disable;
	adp-disable;
	status = "okay";

};

&usbh1 {
	vbus-supply = <&reg_usb_h1_vbus>;
	status = "okay";
};

&mipi_csi {
    status = "okay";
    ipu_id = <0>;
    csi_id = <0>;
    v_channel = <0>;
    lanes = <2>;
};
